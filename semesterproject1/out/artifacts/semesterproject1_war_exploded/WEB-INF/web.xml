<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>servlet.test</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pLogin</servlet-name>
        <servlet-class>servlet.pLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pCatalog</servlet-name>
        <servlet-class>servlet.pCatalog</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pSignup</servlet-name>
        <servlet-class>servlet.pSignup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pMyAccount</servlet-name>
        <servlet-class>servlet.pMyAccount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pExecuter</servlet-name>
        <servlet-class>servlet.pExecuter</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pUpload</servlet-name>
        <servlet-class>servlet.pUpload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pDetails</servlet-name>
        <servlet-class>servlet.pDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>changePassword</servlet-name>
        <servlet-class>servlet.changePassword</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>changePassword</servlet-name>
        <url-pattern>/changePassword</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pLogin</servlet-name>
        <url-pattern>/pLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pCatalog</servlet-name>
        <url-pattern>/pCatalog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pSignup</servlet-name>
        <url-pattern>/pSignup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pMyAccount</servlet-name>
        <url-pattern>/pMyAccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pExecuter</servlet-name>
        <url-pattern>/pExecuter</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pUpload</servlet-name>
        <url-pattern>/pUpload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pDetails</servlet-name>
        <url-pattern>/pDetails</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
